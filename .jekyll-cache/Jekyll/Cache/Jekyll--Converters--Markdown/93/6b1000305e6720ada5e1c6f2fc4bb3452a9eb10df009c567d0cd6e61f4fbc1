  // The date and time that the values were read from the Airbnb web site
  "last_modified": "2017-07-15 21:30:31.858764",

  "latitude": "40.749909",

  "longitude": "-73.87688",

  "location": "0101000020E61000000EA14ACD1E7852C0F0FCA204FD5F4440"
